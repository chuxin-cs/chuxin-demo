{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,

        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": {
          "browsers": ["last 2 versions", "not dead"]
        }
      }
    ]
  ],
  "plugins": [
    [
      // 与 babelHelpers: 'runtime' 配合使用
      "@babel/plugin-transform-runtime"
    ]
  ]
}
